<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DiagramView">
        <attr name="backgroundColor" format="color"/>
        <attr name="foregroundColor" format="color"/>
        <attr name="warningBackgroundColor" format="color"/>
        <attr name="warningForegroundColor" format="color"/>
        <attr name="percentageTextColor" format="color"/>
        <attr name="percentageValue" format="integer"/>
        <attr name="percentageTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CaloriesFromOccasionsDiagramView">
        <attr name="breakfastColor" format="color"/>
        <attr name="lunchColor" format="color"/>
        <attr name="dinnerColor" format="color"/>
        <attr name="snacksColor" format="color"/>
        <attr name="breakfastPercent" format="float"/>
        <attr name="lunchPercent" format="float"/>
        <attr name="dinnerPercent" format="float"/>
        <attr name="snacksPercent" format="float"/>
        <attr name="thicknessDiagram" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CaloriesConsumedProgressView">
        <attr name="caloriesConsumed" format="integer"/>
        <attr name="caloriesBudget" format="integer"/>
        <attr name="caloriesActive" format="integer"/>
        <attr name="caloriesWarning" format="integer"/>
        <attr name="caloriesConsumedColor" format="color"/>
        <attr name="caloriesBudgetColor" format="color"/>
        <attr name="caloriesActiveColor" format="color"/>
        <attr name="caloriesWarningColor" format="color"/>
    </declare-styleable>
</resources>